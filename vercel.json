{
  "version": 2,
  "builds": [
    {
      "src": "/backend/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/contact(.*)",
      "dest": "/backend/api/index.js"
    },
    {
      "src": "/api/request(.*)",
      "dest": "/backend/api/index.js"
    },
    {
      "src": "/api/client(.*)",
      "dest": "/backend/api/index.js"
    }
  ]
}
